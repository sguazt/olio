<!-- bp-project.xml: this is the file that should be included by the project      build files. It will figure out whether it is running from inside Netbeans     or command line and include appropriate tasks.      @Author: Inderjeet Singh --><project name="bp-project" default="dummy-default">  <import file="bp-project/main.xml"/>  <target name="dummy-default"/>    <!-- added so each individual app doesn't require a setup task -->  <!-- if setup is required then app specific task has to come before import of this file -->  <target name="setup"/>  <!-- added so each individual app doesn't require a unsetup task -->  <!-- if unsetup is required then app specific task has to come before import of this file -->  <target name="unsetup"/></project>
